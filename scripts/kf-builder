#!/bin/bash
KF=github.com/kkasravi/kf/kf
rm -rf $GOPATH/src/$KF
mkdir $GOPATH/src/$KF

cobra init $KF
cd $GOPATH/src/$KF
cobra add create
cobra add delete
cobra add exec
cobra add logs
cobra add remove
cobra add services
cobra add list -p 'servicesCmd'
cobra add health -p 'servicesCmd'
cobra add stats -p 'servicesCmd'
cobra add shell
cobra add use
cobra add whoami
cobra add user
cobra add add -p 'userCmd' 
cobra add rm -p 'userCmd' 
cobra add ls -p 'userCmd'
cobra add command
cd -
mv $GOPATH/src/$KF/* .
rmdir $GOPATH/src/$KF
